{
  "name": "openapi-cop-mock-server",
  "version": "1.0.0",
  "description": "Mock server for use together with the openapi-cop proxy server",
  "license": "MIT",
  "author": "Alexis Luengas",
  "private": true,
  "main": "./build/src/app",
  "bin": "./build/src/cli.js",
  "scripts": {
    "postinstall": "npm run compile",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "start": "node build/src/cli.js",
    "pretest": "npm run compile",
    "test": "jest --forceExit --detectOpenHandles --maxWorkers=10",
    "test:deps": "depcheck",
    "posttest": "npm run check",
    "dev": "npm run compile && npm start -- --port 9000 --file \"../schemas/v3/6-examples.yaml\""
  },
  "dependencies": {
    "chalk": "4.1.0",
    "chokidar": "3.4.1",
    "commander": "4.1.1",
    "debug": "4.1.1",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "json-schema-ref-parser": "9.0.3",
    "js-yaml": "3.14.0",
    "morgan": "1.10.0",
    "openapi-backend": "3.5.1",
    "source-map-support": "0.5.19",
    "which": "2.0.2"
  },
  "devDependencies": {
    "@types/errorhandler": "1.5.0",
    "@types/jest": "26.0.4",
    "@types/js-yaml": "3.12.5",
    "@types/json-schema": "7.0.5",
    "@types/morgan": "1.9.1",
    "@types/node": "11.15.5",
    "@types/swagger-parser": "4.0.3",
    "@types/wait-on": "4.0.0",
    "@types/which": "1.3.2",
    "axios": "0.19.2",
    "depcheck": "1.0.0",
    "gts": "1.1.2",
    "jest": "26.1.0",
    "ts-jest": "26.1.2",
    "tslint": "6.1.2",
    "typescript": "3.9.6",
    "wait-on": "5.1.0"
  }
}
