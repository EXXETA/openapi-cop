{
  "name": "openapi-cop-mock-server",
  "version": "0.0.1",
  "description": "Mock server for use together with the openapi-cop proxy server",
  "license": "MIT",
  "author": "Alexis Luengas <alexis.luengas.ruiz@exxeta.com>",
  "main": "./build/src/app",
  "bin": "./build/src/cli.js",
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "start": "node build/src/cli.js",
    "pretest": "npm run compile",
    "test": "jest --forceExit --detectOpenHandles --maxWorkers=10",
    "test:deps": "depcheck",
    "posttest": "npm run check",
    "dev": "npm run compile && npm start -- --port 9000 --file \"../schemas/v3/6-examples.yaml\""
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "chokidar": "^3.0.1",
    "commander": "^2.20.0",
    "debug": "^4.1.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "json-schema-ref-parser": "^6.1.0",
    "js-yaml": "^3.13.1",
    "morgan": "^1.9.1",
    "openapi-backend": "^2.2.0",
    "source-map-support": "^0.5.10",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "@types/errorhandler": "0.0.32",
    "@types/jest": "^24.0.11",
    "@types/js-yaml": "^3.12.1",
    "@types/json-schema": "^7.0.3",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.11.0",
    "@types/swagger-parser": "^4.0.3",
    "@types/wait-on": "^3.2.0",
    "@types/which": "^1.3.1",
    "axios": "^0.18.0",
    "depcheck": "^0.8.3",
    "gts": "^0.9.0",
    "jest": "^24.3.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333",
    "wait-on": "^3.2.0"
  }
}
