{
  "name": "openapi-cop-mock-server",
  "version": "1.0.0",
  "description": "Mock server for use together with the openapi-cop proxy server",
  "license": "MIT",
  "author": "Alexis Luengas",
  "private": true,
  "main": "./build/src/app",
  "bin": "./build/src/cli.js",
  "scripts": {
    "postinstall": "npm run compile",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "start": "node build/src/cli.js",
    "pretest": "npm run compile",
    "test": "jest --forceExit --detectOpenHandles --maxWorkers=10",
    "test:deps": "depcheck",
    "posttest": "npm run check",
    "dev": "npm run compile && npm start -- --port 9000 --file \"../schemas/v3/6-examples.yaml\""
  },
  "dependencies": {
    "chalk": "2.4.2",
    "chokidar": "3.3.1",
    "commander": "2.20.3",
    "debug": "4.1.1",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "json-schema-ref-parser": "6.1.0",
    "js-yaml": "3.13.1",
    "morgan": "1.9.1",
    "openapi-backend": "2.3.5",
    "source-map-support": "0.5.16",
    "which": "1.3.1"
  },
  "devDependencies": {
    "@types/errorhandler": "0.0.32",
    "@types/jest": "25.1.4",
    "@types/js-yaml": "3.12.2",
    "@types/json-schema": "7.0.4",
    "@types/morgan": "1.9.0",
    "@types/node": "11.15.5",
    "@types/swagger-parser": "4.0.3",
    "@types/wait-on": "3.2.0",
    "@types/which": "1.3.2",
    "axios": "0.19.2",
    "depcheck": "0.9.2",
    "gts": "0.9.0",
    "jest": "24.9.0",
    "ts-jest": "25.2.1",
    "tslint": "5.20.1",
    "typescript": "3.8.3",
    "wait-on": "3.3.0"
  }
}
