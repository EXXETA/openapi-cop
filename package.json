{
  "name": "openapi-cop",
  "version": "0.0.3",
  "description": "OpenAPI compliance proxy",
  "author": "Alexis Luengas",
  "repository": {
    "type": "git",
    "url": "github:EXXETA/openapi-cop"
  },
  "keywords": [
    "openapi",
    "swagger",
    "proxy",
    "monitoring",
    "validation",
    "validator"
  ],
  "files": [
    "build/"
  ],
  "license": "MIT",
  "main": "./build/src/app",
  "bin": "./build/src/cli.js",
  "types": "./build/src/app.d.ts",
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "start": "node build/src/cli",
    "dev": "npm run compile && node build/src/cli --port 3000 --target \"http://localhost:8889\" --file \"./schemas/v3/6-examples.yaml\"",
    "dev-schema": "npm run compile && node build/src/cli --port 3000 --target \"http://localhost:8889\" --file",
    "dev-mock": "(export DEBUG=openapi-cop:* || set DEBUG=openapi-cop:*) && npm run compile && node build/test/util/spawn",
    "pretest": "npm run compile",
    "test": "mocha --bail --timeout 60000 --exit build/test/*.test.js",
    "test:docker": "bash ./docker/run-docker-test.bash",
    "test:deps": "npx depcheck --ignores=\"@types/*\"",
    "posttest": "npm run check",
    "prepare": "npm run compile",
    "package": "npx yarn pack --filename openapi-cop.tgz"
  },
  "dependencies": {
    "@types/express": "4.16.1",
    "ajv": "6.10.2",
    "api-spec-converter": "2.7.32",
    "chalk": "3.0.0",
    "chokidar": "2.1.2",
    "commander": "2.19.0",
    "content-type": "1.0.4",
    "debug": "4.1.1",
    "express": "4.16.4",
    "js-yaml": "3.13.1",
    "json-schema-ref-parser": "6.1.0",
    "lodash": "4.17.15",
    "openapi-backend": "2.3.4",
    "openapi-types": "1.3.4",
    "qs": "6.6.0",
    "request-promise-native": "1.0.7",
    "swagger-client": "3.8.25",
    "swagger-parser": "6.0.5",
    "wait-on": "3.2.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/content-type": "1.1.3",
    "@types/debug": "4.1.4",
    "@types/js-yaml": "3.12.0",
    "@types/json-schema": "7.0.3",
    "@types/lodash.merge": "4.6.6",
    "@types/mocha": "5.2.6",
    "@types/node": "10.0.3",
    "@types/qs": "6.5.2",
    "@types/request-promise-native": "1.0.15",
    "@types/swagger-parser": "4.0.3",
    "@types/wait-on": "3.2.0",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "axios": "0.18.0",
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "find-process": "1.2.1",
    "gts": "1.1.2",
    "mocha": "6.0.2",
    "openapi-cop-mock-server": "file:./mock",
    "tslint": "5.18.0",
    "typescript": "3.8.2"
  }
}
